{
  "name": "mssql-handler",
  "version": "1.0.6",
  "description": "MSSQL-HANDLER is a node module that will provide an easy way to access queries and procedures in a MS SQL server",
  "main": "./lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rmdir /s /q lib",
    "build": "npm run clean && babel ./src --ignore node_modules,.git,.babelrc,bin,lib -d ./lib",
    "start": "npm run build && node ./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Koushik-Sarkar/mssql-handler.git"
  },
  "author": "Koushik Sarkar (https://www.linkedin.com/in/koushik-sarkar/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Koushik-Sarkar/mssql-handler/issues"
  },
  "homepage": "https://github.com/Koushik-Sarkar/mssql-handler/issues",
  "dependencies": {
    "mssql": "^4.0.4",
    "tedious": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2"
